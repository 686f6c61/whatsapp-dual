{
  "name": "whatsapp-dual",
  "version": "1.1.8.1",
  "description": "WhatsApp Personal and Business in a single desktop app",
  "main": "src/main/main.js",
  "author": {
    "name": "686f6c61",
    "email": "686f6c61@users.noreply.github.com"
