  "license": "MIT",
  "homepage": "https://github.com/686f6c61/whatsapp-dual",
  "repository": {
    "type": "git",
    "url": "https://github.com/686f6c61/whatsapp-dual.git"
  },
  "bugs": {
    "url": "https://github.com/686f6c61/whatsapp-dual/issues"
  },
  "keywords": [
    "whatsapp",
    "whatsapp-web",
    "whatsapp-business",
    "electron",
    "desktop",
    "linux",
    "ubuntu"
  ],
  "scripts": {
    "start": "electron . --no-sandbox",
    "dev": "electron . --no-sandbox --enable-logging",
    "build": "electron-builder --config build/electron-builder.yml",
    "build:linux": "electron-builder --config build/electron-builder.yml --linux",
    "build:deb": "electron-builder --config build/electron-builder.yml --linux deb",
    "build:appimage": "electron-builder --config build/electron-builder.yml --linux AppImage",
    "build:snap": "electron-builder --config build/electron-builder.yml --linux snap",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.9"
  },
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^25.1.8"
  }
}
